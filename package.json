{
  "name": "mu-pdf-viewer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azu/mu-pdf-viewer.git"
  },
  "author": "azu",
  "email": "azuciao@gmail.com",
  "homepage": "https://github.com/azu/mu-pdf-viewer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azu/mu-pdf-viewer/issues"
  },
  "files": [
    "src/",
    "lib/",
    "bin/",
    "public/"
  ],
  "version": "1.3.0",
  "description": "pdf viewer on electron",
  "main": "index.js",
  "bin": {
    "mu-pdf-viewer": "./bin/cmd.js"
  },
  "build": {
    "productName": "mu-pdf-viewer",
    "appId": "info.efcl.mu-pdf-viewer",
    "asar": true,
    "npmRebuild": false,
    "files": [
      "index.js",
      "public/**/*",
      "bin/**/*",
      "node_modules/**/*"
    ],
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "scripts": {
    "electron": "electron index.js",
    "start": "npm-run-all clean --parallel watch:* electron",
    "test": "mocha test/",
    "build": "npm-run-all clean --parallel build:*",
    "watch": "npm-run-all clean --parallel watch:*",
    "prepublish": "npm run --if-present build",
    "clean": "rimraf public/build dist && mkdirp public/build dist",
    "build:js": "webpack --mode production",
    "watch:js": "webpack --mode development --watch",
    "pack": "electron-builder --dir",
    "predist": "npm run build",
    "dist": "electron-builder"
  },
  "keywords": [
    "pdf",
    "electron"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "babel-preset-jsdoc-to-assert": "^5.0.0",
    "babel-preset-power-assert": "^3.0.0",
    "brfs": "^2.0.2",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^2.1.1",
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.4.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.23.1",
    "transform-loader": "^0.2.3",
    "webpack": "^4.32.1",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "almin": "^0.9.1",
    "almin-logger": "^3.0.0",
    "electron-default-menu": "^1.0.0",
    "electron-gh-releases": "^2.0.4",
    "electron-window-state": "^5.0.3",
    "glob": "^7.0.5",
    "map-like": "^1.0.1",
    "minimist": "^1.2.0",
    "mousetrap": "^1.6.0",
    "normalize.css": "^5.0.0",
    "pdfjs-dist": "1.5.376",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  }
}
